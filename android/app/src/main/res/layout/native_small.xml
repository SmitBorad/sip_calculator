<com.google.android.gms.ads.nativead.NativeAdView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_centerInParent="true"
    android:background="@drawable/ads_stroke">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginVertical="10dp">
            <ImageView
                android:id="@+id/ad_app_icon"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_centerVertical="true"
                android:layout_marginStart="10dp"/>


            <LinearLayout
                android:id="@+id/background"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                android:layout_toStartOf="@+id/ad_call_to_action"
                android:layout_toEndOf="@+id/ad_app_icon"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <androidx.cardview.widget.CardView
                        android:layout_width="25sp"
                        android:layout_height="18sp"
                        app:cardCornerRadius="5dp">
                        <TextView android:layout_width="25sp" android:layout_height="18sp" android:background="#fbb320" android:gravity="center" android:text="Ad" android:paddingHorizontal="5dp" android:textColor="#ffffff" android:textSize="9sp" android:textStyle="bold" app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
                    </androidx.cardview.widget.CardView>
                    <TextView android:id="@+id/ad_headline" android:layout_width="match_parent" android:layout_height="match_parent" android:lines="1" android:maxLines="1" android:layout_marginLeft="5dp" android:textColor="#000000" android:textSize="13sp" android:textStyle="bold"/>
                </LinearLayout>
                <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="vertical">
                    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">
                        <RatingBar android:id="@+id/ad_stars" style="@style/Widget.AppCompat.RatingBar.Small" android:layout_width="wrap_content" android:layout_height="match_parent" android:lines="1" android:numStars="5" android:textColor="#ffffff" android:textSize="12sp" android:visibility="gone"></RatingBar>
                        <TextView android:id="@+id/ad_advertiser" android:layout_width="match_parent" android:layout_height="match_parent" android:lines="1" android:maxLines="1" android:textColor="#000000" android:textSize="9sp"/>
                    </LinearLayout>
                    <TextView android:id="@+id/ad_body" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_gravity="center" android:layout_marginTop="1dp" android:text="" android:textColor="#000000" android:textSize="9sp"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:id="@+id/ad_price" android:layout_width="0dp" android:layout_height="0dp" android:textSize="12sp"/>
            <TextView android:id="@+id/ad_store" android:layout_width="0dp" android:layout_height="0dp" android:textSize="12sp"/>
            <Button android:id="@+id/ad_call_to_action" android:layout_width="90dp" android:layout_height="35dp" android:layout_alignParentEnd="true" android:layout_centerVertical="true" android:layout_marginHorizontal="10dp" android:background="@drawable/ads_button" android:lines="1" android:text="Install Now" android:textColor="#ffffff" android:textStyle="bold" android:textSize="12sp"/>
        </RelativeLayout>
    </RelativeLayout>
</com.google.android.gms.ads.nativead.NativeAdView>
